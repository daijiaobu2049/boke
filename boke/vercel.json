{
    "version": 2,
    "builds": [
        {
            "src": "blog-backend/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/blog-backend/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
} 